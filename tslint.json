{
  "extends": "tslint:recommended",
  "rules": {
    "max-line-length": {
      "options": [
        80
      ]
    },
    "new-parens": true,
    "no-arg": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": false,
    "semicolon": [true, "never"],
    "member-access": false,
    "one-line": false
  },
  "jsRules": {
    "max-line-length": {
      "options": [
        80
      ]
    }
  }
}